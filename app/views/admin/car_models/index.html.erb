<%= link_to 'New Car Model', new_admin_car_model_path %>
<br/><br/>

<table class="admintable">
 <tr class="admintablehd">
    <td width="25%">Name</td>
    <td width="25%">Car Type</td>
   <td colspan=2 style="text-align: center !important;">Actions</td>
</tr>



<% @car_models.each do |car_model|%>
  <tr>
    <td><%= car_model.name rescue nil %></td>
    <td><%= car_model.car_make.name rescue nil %></td>
    <td style="text-align: center !important;"><%= link_to 'Edit', edit_admin_car_model_path(car_model) %></td>
    <td style="text-align: center !important;"><%= link_to 'Destroy',admin_car_model_path(car_model) ,:method => :delete , :data=> { :confirm=> 'Are you sure?' } %>
    </td>
  </tr>
<% end %>
</table>

<br />

