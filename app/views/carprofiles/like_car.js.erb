<% if current_user.likes?(@car_profile) %>
jQuery("#like_car img").attr("src","/assets/like.jpg");
jQuery("#like_count").html('<%= "#{@counts}"%>');
jQuery("#star_rating").attr("class",'<%=@class%>');
<% else %>
jQuery("#like_car img").attr("src","/assets/like.jpg");
jQuery("#like_count").html('<%= "#{Like.sum('count')
} likes."%>');
<% end %>