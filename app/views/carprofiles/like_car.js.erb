<% if current_user.likes?(@car_profile) %>
$("#like_car img").attr("src","/assets/like.jpg");
$("#like_count").html('<%= "#{@counts}"%>');
$("#star_rating").attr("class",'<%=@class%>');
<% else %>
$("#like_car img").attr("src","/assets/like.jpg");
$("#like_count").html('<%= "#{Like.sum('count')
} likes."%>');
<% end %>