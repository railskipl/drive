
<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
"http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html>
<head>
	<meta http-equiv="content-type" content="text/html;charset=UTF-8" />
	  <title>Admin: <%= controller.action_name %></title>
  <%= stylesheet_link_tag  'admin/admin' %>
  <%= javascript_include_tag  "application","jquery.min.js","nested",'nicEdit'%>
  <%= csrf_meta_tags %>

</head>

<body>
	<div id="header">
		<div id="logo">
			<%= image_tag  "/assets/logo.png" %>&nbsp; <span style="color:#fff; font-size:34px;">Admin Panel</span>
		</div>
		<div id="nav">
				<ul class="tabs">
		
<li><% if controller.controller_name == 'pages' %> <%= link_to "Pages", admin_pages_path, :class=>"active" %> <% else %> <%= link_to "Pages", admin_pages_path %> <% end %></li>

<li><% if controller.controller_name == 'seos' %> <%= link_to "Seo", admin_seos_path, :class=>"active" %> <% else %> <%= link_to "Seo",admin_seos_path %> <% end %></li>

<li><% if controller.controller_name == 'blogs' %> <%= link_to "Blog Topics", admin_blogs_path , :class=>"active" %> <% else %> <%= link_to "Blog Topics", admin_blogs_path %> <% end %></li>

<li><% if controller.controller_name == 'car_makes' %> <%= link_to "Car Make", admin_car_makes_path, :class=>"active" %> <% else %> <%= link_to "Car Make", admin_car_makes_path %> <% end %></li>

<li><% if controller.controller_name == 'car_models' %> <%= link_to "Car Model", admin_car_models_path, :class=>"active" %> <% else %> <%= link_to "Car Model",  admin_car_models_path%> <% end %></li>
<li><% if controller.controller_name == 'credits' %> <%= link_to "Credit Conversion", admin_credits_path, :class=>"active" %> <% else %> <%= link_to "Credit Conversion",  admin_credits_path %> <% end %></li>
<li><% if controller.controller_name == 'engines' %> <%= link_to "Engine ", admin_engines_path, :class=>"active" %> <% else %> <%= link_to "Engine",  admin_engines_path %> <% end %></li>
<li><% if controller.controller_name == 'admincontacts' %> <%= link_to "Feedback ", admincontacts_path, :class=>"active" %> <% else %> <%= link_to "Feedback",  admincontacts_path %> <% end %></li>
<li><% if controller.controller_name == 'body_indices' %> <%= link_to "Body Index ", admin_body_indices_path, :class=>"active" %> <% else %> <%= link_to "Body Index ",  admin_body_indices_path %> <% end %></li>
<li>
	<% if controller.controller_name == 'egifts' %> 
	<%= link_to "Egifts", admin_egifts_path, :class=>"active" %> 
	<% else %> 
	<%= link_to "Egifts",  admin_egifts_path%> <% end %>
</li>

<li><% if controller.controller_name == 'logbook_categories' %> <%= link_to "Logbook Categories", admin_logbook_categories_path , :class=>"active" %> <% else %> <%= link_to "Logbook Categories", admin_logbook_categories_path%> <% end %></li>


<li><% if controller.controller_name == 'users' %> <%= link_to "User", users_path , :class=>"active" %> <% else %> <%= link_to "Users", users_path%> <% end %></li>



	    <li>
					<% if admin_signed_in? %> 
						<%= link_to "Sign Out", destroy_admin_session_path,:method => :delete %> 
					<% else %> 
						<%= link_to "Sign In", new_admin_session_path %> &nbsp; 
					<% end %>
			</li>	
			</ul>
		
		</div>
		</div>

	<div id='container'>
		<div id="main">
			<p style="color: green"><%= flash[:notice] %></p>
	
  			<%= yield %>
  		</div>
	</div>
	
	<div id="footer">
      <div id="footer_logo">
            <div id="powered_by">
            	Powered by <a target="_blank" href="http://www.kunalinfotech.net" style="color:#FFFFFF;">Kunalinfotech Pvt Ltd</a>
        	</div>
      </div>
    </div>

</body>
</html>
		


