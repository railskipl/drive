<!DOCTYPE html>
<html>
<head>
  <title><%= @meta_title %></title>
  <meta name="keyword" content="<%= @meta_keyword %>" />
  <meta name="description" content="<%= @meta_description %>" />

  <%= stylesheet_link_tag  "reset", "text", "style", "1000_20_10_10","jsCarousel-2.0.0" %>

  <%= javascript_include_tag "application" ,"jquery-1.5.2.min","jquery.validate.min","validation","jsCarousel-2.0.0" %>
  

  <%= javascript_include_tag "nicEdit" %>

  <%#= javascript_include_tag :defaults %>

<%= stylesheet_link_tag "http://ajax.googleapis.com/ajax/libs/jqueryui/1.7.2/themes/redmond/jquery-ui.css" %>
<%#= javascript_include_tag  "http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.1/jquery-ui.min.js" %>
<%= csrf_meta_tags %>


<link rel="shortcut icon" type="image/x-icon" href="favicon.ico" />
</head>
<body>

<div class="wraper">


<%= render 'shared/header' %>
<div id="flash_notice"> 
  <%= flash[:notice] %>
</div>
<div id="flash_error"> 
  <%= flash[:error] %>
</div>   
<% if user_signed_in? %>
   <%= render 'shared/top_menu' %>
<% end %>
<div class="container_20">

<%= yield %>

</div>
<%= render 'shared/top_cars' %>

<%= render 'shared/footer' %>
</div>





</body>
</html>
