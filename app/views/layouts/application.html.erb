<!DOCTYPE html>
<html>
<head>
  <title><%= @meta_title %></title>
  <meta name="keyword" content="<%= @meta_keyword %>" />
  <meta name="description" content="<%= @meta_description %>" />

  <%= stylesheet_link_tag  "reset", "text", "style", "1000_20_10_10","jsCarousel-2.0.0", "pagination","jquery.fancybox-1.3.4.pack" %>

  <%= javascript_include_tag "application" ,"jsCarousel-2.0.0","script.js" ,"nested","jquery.easy-ticker","jquery.fancybox-1.3.4.pack" %>
  
 <%= javascript_include_tag 'nicEdit'%>

  <%#= javascript_include_tag :defaults %>

<%= stylesheet_link_tag "http://ajax.googleapis.com/ajax/libs/jqueryui/1.7.2/themes/redmond/jquery-ui.css" %>
<%#= javascript_include_tag  "http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.1/jquery-ui.min.js" %>
<%= csrf_meta_tags %>


<link rel="shortcut icon" type="image/x-icon" href="favicon.ico" />
</head>
<body>



<%= render 'shared/header' %>
  <% if flash[:notice] %>

<div id="flash_notice"> 
  <%= flash[:notice] %>
</div>
<% end %>
   

<% if user_signed_in? %>
   <%= render 'shared/top_menu' %>
<% end %>
<div class="container_20">

<%= yield %>

</div>
<%= render 'shared/top_cars' %>

<%= render 'shared/footer' %>






</body>
</html>
