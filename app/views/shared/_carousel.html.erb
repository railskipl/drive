<script type="text/javascript">
jQuery.noConflict();
jQuery(document).ready(function() {
 jQuery('#carouselhAuto').jsCarousel({ onthumbnailclick: function(src) { }, autoscroll: true, masked: true, itemstodisplay: 5, orientation: 'h' });
});       
</script>
<div id="carouselhAuto">
   <% @spotlighted_cars.each do |car|%>
    <% car.carprofile_photos.each do |photo_url|%>
    <div>
     <%= link_to (image_tag photo_url.photo.url(:medium)),carprofile_path(car)%><br/>
      <span class="thumbnail-text"><%= car.car_make.name %></span>
      </div>
    <% end %>
   <% end %>              
</div>